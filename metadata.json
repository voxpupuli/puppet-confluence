{
  "name": "puppet-confluence",
  "version": "7.0.0",
  "author": "Vox Pupuli",
  "summary": "Install atlassian confluence",
  "license": "MIT",
  "source": "https://github.com/voxpupuli/puppet-confluence.git",
  "project_page": "https://github.com/voxpupuli/puppet-confluence",
  "issues_url": "https://github.com/voxpupuli/puppet-confluence/issues",
  "description": "Atlassian confluence",
  "dependencies": [
    {
      "name": "puppet/archive",
      "version_requirement": ">= 1.0.0 < 9.0.0"
    },
    {
      "name": "puppet/mysql_java_connector",
      "version_requirement": ">= 3.0.2 < 7.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 10.0.0"
    }
  ],
  "requirements": [
    {
      "name": "openvox",
      "version_requirement": ">= 8.19.0 < 9.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "8",
        "9"
